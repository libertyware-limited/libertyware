{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "youth-defunder": {
      "root": "apps/youth-defunder/",
      "sourceRoot": "apps/youth-defunder/src",
      "projectType": "application",
      "prefix": "ph",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/youth-defunder",
            "index": "apps/youth-defunder/src/index.html",
            "main": "apps/youth-defunder/src/main.ts",
            "polyfills": "apps/youth-defunder/src/polyfills.ts",
            "tsConfig": "apps/youth-defunder/tsconfig.app.json",
            "assets": ["apps/youth-defunder/src/favicon.ico", "apps/youth-defunder/src/assets"],
            "styles": ["apps/youth-defunder/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/youth-defunder/src/environments/environment.ts",
                  "with": "apps/youth-defunder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "youth-defunder:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "youth-defunder:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "youth-defunder:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/youth-defunder/tsconfig.app.json", "apps/youth-defunder/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/youth-defunder/jest.config.js",
            "tsConfig": "apps/youth-defunder/tsconfig.spec.json",
            "setupFile": "apps/youth-defunder/src/test-setup.ts"
          }
        }
      }
    },
    "youth-defunder-e2e": {
      "root": "apps/youth-defunder-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/youth-defunder-e2e/cypress.json",
            "tsConfig": "apps/youth-defunder-e2e/tsconfig.e2e.json",
            "devServerTarget": "youth-defunder:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "youth-defunder:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/youth-defunder-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "angular-ng-sentry": {
      "root": "libs/angular/ng-sentry",
      "sourceRoot": "libs/angular/ng-sentry/src",
      "projectType": "library",
      "prefix": "libertyware",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/angular/ng-sentry/tsconfig.lib.json",
            "project": "libs/angular/ng-sentry/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/angular/ng-sentry/tsconfig.lib.json", "libs/angular/ng-sentry/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "youth-defunder"
}
